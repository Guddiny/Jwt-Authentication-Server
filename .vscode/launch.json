{
    // Use IntelliSense to find out which attributes exist for C# debugging
    // Use hover for the description of the existing attributes
    // For further information visit https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
        {
            "name": "AuthServer",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build authserver",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/AuthServer/bin/Debug/netcoreapp3.1/AuthServer.dll",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "justMyCode": false,
            "args": [],
            "cwd": "${workspaceFolder}/AuthServer",
            "stopAtEntry": false,
            "enableStepFiltering": false
        },
        {
            "name": "ProtectedAPIResource",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build protectedapiresource",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}\\Resources Server\\ProtectedAPIResource\\bin\\Debug\\netcoreapp3.1\\ProtectedAPIResource.dll",
            "args": [],
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "justMyCode": false,
            "cwd": "${workspaceFolder}\\Resources Server\\ProtectedAPIResource",
            "stopAtEntry": false,
            "enableStepFiltering": false
        }
    ],
    "compounds": [
        {
            "name": "AuthServer/ProtectedAPIResource",
            "configurations": [
                "AuthServer",
                "ProtectedAPIResource"
            ]
        }
    ]
}